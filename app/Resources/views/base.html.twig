<!DOCTYPE html>
{% block html_tag %}<html>{% endblock %}
    <head>
        {% block metas %}
        <meta charset="UTF-8" />
        {% endblock %}
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {% block favicon %}
        <link rel="icon" type="image/png" href="{{ asset('favicon.ico') }}" />
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
