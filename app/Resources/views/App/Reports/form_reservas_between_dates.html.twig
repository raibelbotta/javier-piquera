<div class="modal fade" data-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content">
            {{ form_start(form, {'action': path('app_reports_all_reservas_between_dates'), 'attr': {'role': 'form', 'target': '_blank'}}) }}
                <div class="modal-header">
                    <button class="close" type="button" data-dismiss="modal">&times</button>
                    <h4 class="modal-title">Servicios entre fechas</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-6">
                            {{ form_row(form.fromDate) }}
                        </div>
                        <div class="col-lg-6">
                            {{ form_row(form.toDate) }}
                        </div>
                    </div>
                    {{ form_row(form.services) }}
                    <div class="row">
                        <div class="col-lg-6">
                            {{ form_row(form.includePlacesAddress) }}
                        </div>
                        <div class="col-lg-6">
                            {{ form_row(form.showProviderLogoIfPossible) }}
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-success" type="submit">
                        <i class="fa fa-print"></i>
                        Generar
                    </button>
                    <button class="btn btn-default" data-dismiss="modal" type="button">
                        Cerrar
                    </button>
                </div>
            {{ form_end(form) }}
        </div>
    </div>
</div>
<script>
    $(document).ready(function() {
        $('#{{ form.services.vars.id }}').select2({width: '100%'});
        $('#{{ form.fromDate.vars.id }}, #{{ form.toDate.vars.id }}').datepicker({format: 'dd/mm/yyyy', autoclose: true});
        $('#{{ form.includePlacesAddress.vars.id }}, #{{ form.showProviderLogoIfPossible.vars.id }}').iCheck({
            checkboxClass: 'icheckbox_flat-blue'
        });
    });
</script>
