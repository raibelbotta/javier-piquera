{{ form_start(form, {'action': path('app_reports_all_reservas_by_provider'), 'attr': {'role': 'form', 'target': '_blank'}}) }}
    <div class="modal-header">
        <button class="close" type="button" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Servicios por agencia</h4>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-lg-6">
                {{ form_label(form.fromDate) }}
                {{ form_widget(form.fromDate, {'attr': {'class': 'form-control datepicker'}}) }}
                <div class="help-block"></div>
            </div>
            <div class="col-lg-6">
                {{ form_label(form.toDate) }}
                {{ form_widget(form.toDate, {'attr': {'class': 'form-control datepicker'}}) }}
                <div class="help-block"></div>
            </div>
            <div class="col-xs-12">
                {{ form_label(form.provider) }}
                {{ form_widget(form.provider, {'attr': {'class': 'form-control'}}) }}
                <div class="help-block"></div>
            </div>
            <div class="col-xs-12">
                {{ form_label(form.services, "Servicios") }}
                {{ form_widget(form.services) }}
                <div class="help-block"></div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button class="btn btn-success" type="submit">
            <i class="fa fa-print"></i>
            Generar
        </button>
        <button class="btn btn-default" data-dismiss="modal" type="button">
            Cerrar
        </button>
    </div>
{{ form_end(form) }}

<script>
    $(document).ready(function() {
        $('#{{ form.services.vars.id }}, #{{ form.provider.vars.id }}').select2({'width': '100%'});
        $('#{{ form.toDate.vars.id }}, #{{ form.fromDate.vars.id }}').datepicker({
            dateFormat: 'dd/mm/yy'
        });
    });
</script>
