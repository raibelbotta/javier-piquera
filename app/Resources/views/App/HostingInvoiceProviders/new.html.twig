{% extends "frontend-requirejs.html.twig" %}

{% form_theme form "App/HostingInvoiceProviders/form.html.twig" %}

{% block page_content %}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Proveedores de hospedaje <small>nuevo</small></h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->

    {{ form_start(form, {action: path("app_hostinginvoiceproviders_new"), attr: {role: "form"}}) }}
        <div class="row">
            <div class="col-lg-12">
                {{ form_row(form) }}
                <button class="btn btn-success" type="submit">
                    <i class="fa fa-save"></i>
                    Guardar
                </button>
                <a href="{{ path("app_hostinginvoiceproviders_index") }}" class="btn btn-default">
                    <i class="fa fa-undo"></i>
                    Cancelar
                </a>
            </div>
        </div>
    <!-- /.row -->
    {{ form_end(form) }}
{% endblock %}
